{
  "name": "createqr",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun build index.ts --compile --outfile dist/createqr",
    "build:linux": "bun build index.ts --compile --target=bun-linux-x64 --outfile dist/createqr-linux-x64",
    "build:macos-x64": "bun build index.ts --compile --target=bun-darwin-x64 --outfile dist/createqr-macos-x64",
    "build:macos-arm64": "bun build index.ts --compile --target=bun-darwin-arm64 --outfile dist/createqr-macos-arm64"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "pngjs": "^7.0.0",
    "qrcode-generator": "^2.0.4"
  }
}
